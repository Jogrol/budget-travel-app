type Post {
  id: ID! @id
  published: Boolean! @default(value: false)
  title: String!
  content: String!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type Expense {
  id: ID! @id
  description: String!
  amount: Int!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
  category: String!
  subcategory: String
  start_date: DateTime!
  end_date: DateTime
  days: Float!
}